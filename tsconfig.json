{
    "compilerOptions": {
        "outDir": "./dist/",
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "dom",
            "es5",
            "es2015.collection",
            "es2015.iterable",
            "es2017"
        ],
        "allowJs": false,
        "downlevelIteration": true,
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules/**/*.ts"
    ]
}